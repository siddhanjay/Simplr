    "tslint": "^5.7.0",
    "typescript": "^2.5.3",
    "babel-jest": "^23.4.2",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react-scripts": "^1.1.5"
  },
